,CAAA,CAAA,IAAA,EAAA,EAAA,OAAA,CAAA,GAAA,IAAA,CAAA,IAAA,yBADA;AAAA,SAAA;;AAGA,QAAA,KAAA,CAAA,IAAA,CAAA,IAAA;AACA;;AAEA,UAAA,KAAA,KAAA,EAAA;AACA,YAAA,KAAA,GAAA,SAAA,KAAA,CAAA,CAAA;AAAA,iBACA,CAAA,CAAA,CAAA,CAAA,IAAA,CAAA,MAAA,KAAA,CAAA,KAAA,IAAA,8BADA;AAAA,SAAA;;AAGA,QAAA,KAAA,CAAA,IAAA,CAAA,KAAA;AACA;;AAEA,aAAA,KAAA;AACA;AAnBA;AAnBA,CAAA","sourcesContent":["<template>\n  <v-app id=\"container\">\n    <v-container fluid class=\"mx-auto my-auto\">\n    <v-row>\n      <v-col cols=\"12\">\n        <v-row justify=\"center\">\n          <v-card elevation=\"10\" class=\"mt-5 px-4\" shaped>\n            <v-card-title>Crear cuenta</v-card-title>\n            <v-card-text>\n              <v-form>\n                <v-text-field\n                  label=\"Nombre\"\n                  prepend-icon=\"mdi-account-circle\"\n                  type=\"name\"\n                  counter=\"20\"\n                  :rules=\"nameRules\"\n                ></v-text-field>\n                <v-text-field\n                  label=\"E-mail\"\n                  prepend-icon=\"mdi-email\"\n                  type=\"email\"\n                  :rules=\"emailRules\"\n                ></v-text-field>\n                <v-text-field\n                  :append-icon=\"show1 ? 'mdi-eye-off' : 'mdi-eye'\"\n                  :type=\"show1 ? 'password' : 'text'\"\n                  name=\"input-10-2\"\n                  label=\"Contraseña\"\n                  class=\"input-group--focused\"\n                  @click:append=\"show1 = !show1\"\n                  prepend-icon=\"mdi-lock\"\n                  v-model=\"match\"\n                ></v-text-field>\n                <v-text-field\n                  :append-icon=\"show2 ? 'mdi-eye-off' : 'mdi-eye'\"\n                  :type=\"show2 ? 'password' : 'text'\"\n                  name=\"input-10-2\"\n                  label=\"Confirmar contraseña\"\n                  class=\"input-group--focused\"\n                  @click:append=\"show2 = !show2\"\n                  prepend-icon=\"mdi-lock\"\n                  :rules=\"rules\"\n                ></v-text-field>\n                <div class=\"py-3\">\n                  <v-btn block color=\"info\" id=\"ingresar\" rounded type=\"submit\">Crear</v-btn>\n                </div>\n                <center class=\"pt-1 pb-1\">\n                  <v-btn text height=\"40px\" rounded id=\"crear\" to=\"/login\">¿Ya tenés cuenta?</v-btn>\n                </center>\n              </v-form>\n            </v-card-text>\n          </v-card>\n        </v-row>\n      </v-col>\n    </v-row>\n  </v-container>\n  </v-app>\n</template>\n\n<script>\nexport default {\n  name: \"CreateAccount\",\n\n  data: () => ({\n    show1: true,\n    show2: true,\n    allowSpaces: false,\n    match: \"\",\n    nameRules: [\n      v => !!v || \"Este campo es obligatorio\",\n      v => (v && v.length <= 20) || \"El nombre debe ser más corto\"\n    ],\n    emailRules: [\n      v => !!v || \"Este campo es obligatorio\",\n      v => /.+@.+\\..+/.test(v) || \"El e-mail debe ser válido\", // algo@algo.algo\n      v => (v || \"\").indexOf(\" \") < 0 || \"El e-mail debe ser válido\" //sin espacios\n    ]\n  }),\n\n  computed: {\n    rules() {\n      const rules = [];\n\n      if (!this.allowSpaces) {\n        const rule = v =>\n          (v || \"\").indexOf(\" \") < 0 || \"No se permiten espacios\";\n\n        rules.push(rule);\n      }\n\n      if (this.match) {\n        const rule = v =>\n          (!!v && v) === this.match || \"Las contraseñas no coinciden\";\n\n        rules.push(rule);\n      }\n\n      return rules;\n    }\n  }\n};\n</script>\n\n<style scoped>\n#container {\n  background: url(\"../assets/background2.jpg\") no-repeat center center fixed;\n  -webkit-background-size: cover;\n  -moz-background-size: cover;\n  -o-background-size: cover;\n  background-size: cover;\n}\n#ingresar {\n  color: white;\n}\n#crear {\n  text-transform: inherit;\n}\n</style>\n"],"sourceRoot":"src/components"}]}